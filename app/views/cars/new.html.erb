  <H1>NEW CAR</H1>
  <%= simple_form_for([@car]) do |f| %>
    <%= f.input :brand %>
    <%= f.input :model %>
    <%= f.input :capacity %>
    <%= f.input :occasion %>
    <%= f.input :price %>
    <%= f.input :description %>
    <%= f.submit "Add vehicule" %>
  <% end %>

<%= link_to 'Go to vehicules', cars_path, class: "btn btn-primary" %>
