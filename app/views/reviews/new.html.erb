<div class="text-light" id="new-review-forms">
  <div class="container">
    <H1>New Review</H1>
    <%= simple_form_for([@car, @review]) do |f| %>
      <div>
        <%= f.input :content %>
        <%= f.input :rating %>
        <%= f.submit "Add review", class:"btn btn-primary" %>
        <%= link_to 'Go to vehicules', cars_path, class: "btn btn-primary" %>
      </div>
    <% end %>
  </div>
</div>
